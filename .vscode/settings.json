{
    "cSpell.words": [
        "- mermaid2",
        "- search",
        "BUMPLESS",
        "codehilite",
        "fontawesome",
        "inlinehilite",
        "ipynb",
        "javascripts",
        "MELSEC",
        "mkdocs",
        "pycache",
        "pymdownx",
        "venv"
    ],
    "yaml.customTags": [
        "!!python/name:mermaid2.fence_mermaid_custom"
    ],
    "cSpell.ignoreWords": [
        "seqlist",
        "superfences"
    ],

    "terminal.integrated.profiles.mac": {
    "BashVenv": {
      "path": "bash",
      "args": ["--rcfile", "${workspaceFolder}/.venv/bin/activate"]
    }
  },
  "terminal.integrated.defaultProfile.mac": "BashVenv"
}

